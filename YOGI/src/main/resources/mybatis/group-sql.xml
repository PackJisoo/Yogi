<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC " - //mybatis.org//DTD Config 3.0//EN" " http://mybatis.org/dtd/mybatis-3-mapper.dtd">
     
<mapper namespace="group">

	<select id="group_select_all" resultType="hashmap" parameterType="hashmap">														
		select * from group_gathering order by gg_no desc															
	</select>

	<select id="group_search" resultType="GroupModel" parameterType="hashmap">
		select * from group_gathering where ${param} order by gg_no desc
	</select>
</mapper>